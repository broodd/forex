<mjml>
  <mj-head>
    <mj-font
      name="Open Sans"
      href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300..800;1,300..800&family=Playfair+Display:ital,wght@0,400..900;1,400..900&display=swap"
    />
    <mj-attributes>
      <mj-all font-family="Open Sans, sans-serif" color="#212529" />
    </mj-attributes>
    <mj-style inline="inline">
      .block { display: block !important; } .title > div{ font-size: 16px !important; font-weight:
      600 !important; line-height: 1.5 !important; } .label > div{ font-size: 14px !important;
      font-weight: 500 !important; line-height: 1.5 !important; } .wrapper { padding: 48px
      !important; } .logo> table> tbody> tr> td> img{ width: 96px !important; height: 46px
      !important; } .full-width-button { cursor: pointer;}
    </mj-style>
    <mj-style>
      @media (max-width:480px) { .wrapper { padding: 16px !important; border-radius: 0 !important; }
      .logo > table> tbody> tr> td> img{ width: 96px !important; height: 46px !important; } .title >
      div{ font-size: 16px !important; } .label > div{ font-size: 14px !important; }
      .full-width-button { width: 100% !important; display: block !important; cursor: pointer;}}
    </mj-style>
  </mj-head>
  <mj-body>
    <mj-raw>
      <!-- Header -->
    </mj-raw>
    <mj-wrapper css-class="wrapper" background-color="#FBFBFB">
      <mj-section padding="0" text-align="center">
        <mj-column padding-bottom="32px" width="96px">
          <mj-image
            align="center"
            src="{{DOMIAN}}//logo.png"
            padding="0"
            css-class="logo"
          ></mj-image>
        </mj-column>
      </mj-section>
      <mj-raw>
        <!-- Main -->
      </mj-raw>
      <mj-section background-color="#FBFBFB" padding="32px 32px 0" text-align="center">
        <mj-column padding="0">
          <mj-text css-class="label" padding="0">Hallo {{NAME}},</mj-text>
          <mj-text css-class="label" padding="0"
            >vielen Dank f√ºr das Vertrauen in mich und meiner 11 Tages Challenge. In den kommenden
            Tagen werden wir gemeinsam viel lernen und am Ende wirst du richtig stolz auf dich
            sein.</mj-text
          >
