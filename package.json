    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "i18next": "^24.2.1",
    "qs": "^6.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.0",
    "react-router-dom": "^7.1.1",
    "react-use": "^17.6.0",
    "socket.io-client": "^4.8.1",
    "zustand": "^5.0.3"
